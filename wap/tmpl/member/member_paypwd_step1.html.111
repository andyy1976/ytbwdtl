<title>安全密码重置</title>
<link rel="stylesheet" type="text/css" href="../../css/base.css">
<link rel="stylesheet" type="text/css" href="../../css/nctouch_member.css">
</head>
<body>
<header id="header">
	<div class="header-wrap">
		<div class="header-l">
			<a href="member_account.html">
				<i class="back"></i>
			</a>
		</div>
		<div class="header-tab"> <a href="javascript:void(0);" class="cur">安全密码找回</a><a href="mobile_paypwd.html">手机找回安全密码</a></div>
	</div>
</header>
<div class="nctouch-main-layout">
<!-- <div class="register-mobile-tip"> 您当前的手机号码是<em id="mobile"></em></div> -->
  <div class="nctouch-inp-con">
    <form action="" method ="">
	<ul class="form-box">
      <li class="form-item">
        <h4>旧密码：</h4>
        <div class="input-box">
          <input id="member_oldpaypwd" name="member_oldpaypwd" maxlength="10" size="10" class="inp" autocomplete="off"  oninput="writeClear($(this));" type="password">
        </div>
      </li>
      <li class="form-item" id="aaaa">
        <h4>新密码：</h4>
        <div class="input-box">
          <input id="member_newpaypwd" name="member_newpaypwd" maxlength="10" size="10" class="inp" value=""  oninput="writeClear($(this));"  type="password">
        </div>
      </li>
    </ul>
    <div class="form-btn"><a href="javascript:void(0);" class="btn" id="nextform">重置安全密码</a></div>
    </form>
  </div>
</div>
<footer id="footer" class="bottom"></footer>
